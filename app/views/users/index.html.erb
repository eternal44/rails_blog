<div class="container">
<div class="pull-right"><%= link_to 'Sign Out', signout_path %></div>


<h1>All users list</h1>
<table class="table well">
	<tr>
		<th>User Name</th>
		<th>Email</th>
		<th>Action</th>

	</tr>

<% @users.each do |user| %>
	<tr>
		<td><%= user.first_name %></td>
		<td><%= user.email %></td>
		<td><%= link_to 'User related blogs', user_path(user) %></td>
	</tr>
<% end %>
</table>

<%= link_to 'See all blogs', blogs_path %>

</div><!--/.container-->