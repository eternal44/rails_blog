<div class="container">

<h1>All blogs list</h1>
<table class="table well">
	<tr>
		<th>Blog Name</th>
		<th>Description</th>
		<th>Action</th>
	</tr>
<div id="accordion">
<% @blogs.each do |blog| %>
	<tr>
		<td><%= blog.name %></td>
		<td><%= blog.description %></td>
	</tr>
<% end %>
</div><!--/#accordion-->
</table>

<%= link_to 'New Blog', new_blog_path %> |
<%= link_to 'Back to Users', users_path %>

</div><!--/.container-->
